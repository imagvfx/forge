<!doctype html>
<html> [
	<head> [
		{{template "style"}}
	]
	<body> [
		{{template "nav" $}}
		<div class="main"> [
			<div class="one"> [
				<h1> [
					{{pathLinks $.Entry.Path}} ({{$.Entry.Type}})
				]
				<h4> [
					History of {{$.Category}} {{$.Name}}
				]
				<div> [
				{{range $log := $.History}}
					<div style="width:40rem;"> [
						<div style="display:flex;justify-content:space-between;align-items:end"> [
							<div style="font-size:0.9rem;color:#222"> [{{$log.User}}]
							<div style="font-size:0.7rem;color:#666"> [{{$log.When.Local.Format "2006/01/02 15:04:05"}}]
						]
						<div style="display:flex"> [
							<pre style="flex:1;border:solid 1px grey;border-radius:5px;padding:8px;"> [{{$log.Value}}]
							<div style="width:2px"> []
						]
					]
					<div style="height:1rem"> []
				{{end}}
				]
			]
		]
	]
]

