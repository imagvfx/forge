<!doctype html>
<html> [
	<head> [
		{{template "style"}}
	]
	<body> [
		{{template "nav" $}}
		<div class="main"> [
			<div class="left"> [
				<h1> [
					{{pathLinks $.Entry.Path}} ({{$.Entry.Type}})
				]
				<h4> [
					Logs
				]
				<div> [
				{{range $log := $.Logs}}
					<div> [{{$log}}]
				{{end}}
				]
			]
		]
	]
]

